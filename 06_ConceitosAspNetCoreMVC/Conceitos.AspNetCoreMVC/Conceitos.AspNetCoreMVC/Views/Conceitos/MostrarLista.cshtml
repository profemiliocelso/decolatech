@model IEnumerable<Produto>

<h1>Lista de Produtos</h1>

<table class="table table-striped">
    <tr>
        <td>Código</td>
        <td>Descrição</td>
        <td>Preço</td>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Codigo</td>
            <td>@item.Descricao</td>
            <td>@item.Preco</td>
        </tr>
    }
</table>

